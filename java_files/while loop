import java.util.Scanner ;
public static void main (String[] args)
{
    int num;
    System.out.println ("Enter any number: ");
    Scanner sc = new Scanner(System.in);
    num = sc.nextInt();
    while (num>=0)
    {
        if(num %2== 0 )
        {
            System.out.println ("Even number");
            break ;
        }
       else 
        {
            System.out.println ("Odd number");
            break ;
        }
    }
}

    