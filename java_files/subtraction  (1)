import java. util.Scanner ;
class subtraction 
{
    public static void main (String[] args)
    {
        int a ,b,c ;
        System.out.println ("Enter any two numbers=");
        Scanner sc = new Scanner (System.in);
         a = sc.nextInt() ;
        b = sc.nextInt ();
        c = a -b ;
         System.out.println ("The subtraction of two numbers ="+c);
    }
}
