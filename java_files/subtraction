class SubtractionAndAssignmentOperator 
{
    public static void main ( String[] args)
    {
        int num1 = 12 ;
        int num2 = 8;
        
  System.out.println ("num1 = 12; num2 = 8");
  
  num1 -= num2 ;
  
  System.out.println ("The result after subtraction  of these number  is : " +num1 );
  
    }
}
  
        